# Uncomment the next line to define a global platform for your project

target 'MF Metal Example' do
  use_frameworks!
  pod 'MoofFoundation', :git => 'https://github.com/MooseFactory/MoofFoundation.git'
  
  pod 'SnapKit'
end

target 'MF Metal iOS' do
  use_frameworks!
  pod 'MoofFoundation', :git => 'https://github.com/MooseFactory/MoofFoundation.git'
  
  pod 'SnapKit'
end
